{
  "symbols": ["BTC/USD", "ETH/USD", "DOGE/USD"],
  "timeframe": "1m",
  "days_back": 30,
  "model_type": "ensemble",
  "initial_cash": 100000,
  "fees": 0.0005,
  "slippage": 0.0005,
  "confidence_threshold": 0.6,
  "min_hold_period": 5,
  "max_hold_period": 60,
  "test_size": 0.2,
  "validation_size": 0.1,
  "n_features": 50,
  "save_results": true,
  "output_dir": "results",
  "walk_forward": {
    "train_period": 252,
    "test_period": 63,
    "step_size": 21
  },
  "sae_features": {
    "use_synthetic": true,
    "feature_weights": "auto",
    "interaction_features": true
  },
  "backtesting": {
    "benchmark_strategies": ["buy_hold", "ema_crossover"],
    "risk_metrics": true,
    "transaction_costs": true,
    "slippage_model": "linear"
  }
}
